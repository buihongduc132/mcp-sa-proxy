{
  "name": "mcp-sa-proxy",
  "version": "1.0.0",
  "description": "Enhanced MCP SuperAssistant Proxy with WebSocket support and advanced gateway features",
  "keywords": [
    "mcp",
    "stdio",
    "sse",
    "http",
    "websocket",
    "stream",
    "mcp-superassistant",
    "gateway",
    "proxy",
    "bridge"
  ],
  "type": "module",
  "bin": {
    "mcp-sa-proxy": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "./tests/run-all-tests.sh",
    "test:websocket": "./tests/websocket/test-websocket-fix.sh",
    "test:metadata": "cd tests/websocket && node test-client-metadata.js",
    "test:admin": "./tests/integration/test-admin-endpoints.sh",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.2",
    "yargs": "^17.7.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.17.1",
    "@types/ws": "^8.18.1",
    "@types/yargs": "^17.0.33",
    "husky": "^9.1.7",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-reports": "^3.1.7",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "prev-modelcontextprotocol-sdk": "npm:@modelcontextprotocol/sdk@1.4.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
